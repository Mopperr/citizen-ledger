{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProposalListResponse",
  "type": "object",
  "required": [
    "proposals"
  ],
  "properties": {
    "proposals": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ProposalResponse"
      }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "ProposalResponse": {
      "type": "object",
      "required": [
        "description",
        "end_height",
        "execute_at",
        "id",
        "proposer",
        "quorum_bps",
        "start_height",
        "status",
        "threshold_bps",
        "title",
        "votes_abstain",
        "votes_against",
        "votes_for",
        "voting_method"
      ],
      "properties": {
        "description": {
          "type": "string"
        },
        "end_height": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "execute_at": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "id": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "proposer": {
          "type": "string"
        },
        "quorum_bps": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "start_height": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "status": {
          "type": "string"
        },
        "threshold_bps": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "title": {
          "type": "string"
        },
        "votes_abstain": {
          "$ref": "#/definitions/Uint128"
        },
        "votes_against": {
          "$ref": "#/definitions/Uint128"
        },
        "votes_for": {
          "$ref": "#/definitions/Uint128"
        },
        "voting_method": {
          "$ref": "#/definitions/VotingMethod"
        }
      },
      "additionalProperties": false
    },
    "Uint128": {
      "description": "A thin wrapper around u128 that is using strings for JSON encoding/decoding, such that the full u128 range can be used for clients that convert JSON numbers to floats, like JavaScript and jq.\n\n# Examples\n\nUse `from` to create instances of this and `u128` to get the value out:\n\n``` # use cosmwasm_std::Uint128; let a = Uint128::from(123u128); assert_eq!(a.u128(), 123);\n\nlet b = Uint128::from(42u64); assert_eq!(b.u128(), 42);\n\nlet c = Uint128::from(70u32); assert_eq!(c.u128(), 70); ```",
      "type": "string"
    },
    "VotingMethod": {
      "description": "Types of voting supported",
      "oneOf": [
        {
          "description": "One credential = one vote",
          "type": "string",
          "enum": [
            "one_person_one_vote"
          ]
        },
        {
          "description": "Quadratic voting: cost of N votes = N^2 tokens",
          "type": "string",
          "enum": [
            "quadratic"
          ]
        }
      ]
    }
  }
}
